// Place your settings in this file to overwrite default and user settings.
{
    "files.exclude": {
        "**/*.js": {
            "when": "$(basename).ts"
        },
        "**/*.html": {
            "when": "$(basename).jade"
        },
        "**/*.js.map": true,
        "typings": true,
        "node_modules": true
    }
}